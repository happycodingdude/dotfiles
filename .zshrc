# -------
# pnpm Aliases
# -------
alias p="pnpm"
alias pi="pnpm install"
alias pa="pnpm add"
alias pd="pnpm dev"
alias pb="pnpm build"
alias pr="pnpm run"
alias psd="pnpm start:dev"

# ----------------------
# Git Aliases
# ----------------------
alias gi="git init"
alias gro="git remote add origin"
alias gc="git checkout"
alias gb="git branch"
alias gba="git branch -a"
alias gac="git add . && git commit -m"
alias gps="git push"
alias gpso="git push -u origin"
alias gs="git status"
alias gp="git pull"
alias gf="git fetch"
alias gl="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr) %C(bold blue)<%an>%Creset %s'"
